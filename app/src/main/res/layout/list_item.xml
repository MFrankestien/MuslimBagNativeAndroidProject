<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <ImageView
        android:id="@+id/circle1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/circle2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/circle4"

        />
    <ImageView
        android:id="@+id/circle2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toEndOf="@id/circle1"
        app:layout_constraintEnd_toStartOf="@id/circle3"
        app:layout_constraintBottom_toTopOf="@id/circle5"
        app:layout_constraintTop_toTopOf="parent"

        />
    <ImageView
        android:id="@+id/circle3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toEndOf="@id/circle2"
        app:layout_constraintBottom_toTopOf="@id/circle6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <ImageView
        android:id="@+id/circle4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/circle2"
        app:layout_constraintTop_toBottomOf="@id/circle1"
        app:layout_constraintBottom_toTopOf="@id/circle7"
        />
    <ImageView
        android:id="@+id/circle5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toEndOf="@id/circle1"
        app:layout_constraintEnd_toStartOf="@id/circle3"
        app:layout_constraintBottom_toTopOf="@id/circle7"
        app:layout_constraintTop_toBottomOf="@id/circle2"

        />
    <ImageView
        android:id="@+id/circle6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toEndOf="@id/circle5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/circle3"
        app:layout_constraintBottom_toTopOf="@id/circle7"

        />
    <ImageView
        android:id="@+id/circle7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/circle2"
        app:layout_constraintTop_toBottomOf="@id/circle4"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <ImageView
        android:id="@+id/circle8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toEndOf="@id/circle1"
        app:layout_constraintEnd_toStartOf="@id/circle3"

        app:layout_constraintTop_toBottomOf="@id/circle5"
        app:layout_constraintBottom_toBottomOf="parent"

        />
    <ImageView
        android:id="@+id/circle9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/sora_name_icon"
        app:layout_constraintStart_toEndOf="@id/circle5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/circle6"
        app:layout_constraintBottom_toBottomOf="parent"

        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/circle2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/circle4"
        android:textSize="28sp"
        android:id="@+id/sura3"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/circle1"
        app:layout_constraintEnd_toStartOf="@id/circle3"
        app:layout_constraintBottom_toTopOf="@id/circle5"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="28sp"
        android:id="@+id/sura2"

        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/circle2"
        app:layout_constraintBottom_toTopOf="@id/circle6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="28sp"
        android:id="@+id/sura1"

        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/circle2"
        app:layout_constraintTop_toBottomOf="@id/circle1"
        app:layout_constraintBottom_toTopOf="@id/circle7"
        android:textSize="28sp"
        android:id="@+id/sura6"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/circle1"
        app:layout_constraintEnd_toStartOf="@id/circle3"
        app:layout_constraintBottom_toTopOf="@id/circle7"
        app:layout_constraintTop_toBottomOf="@id/circle2"
        android:textSize="28sp"
        android:id="@+id/sura5"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/circle5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/circle3"
        app:layout_constraintBottom_toTopOf="@id/circle9"
        android:textSize="28sp"
        android:id="@+id/sura4"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/circle2"
        app:layout_constraintTop_toBottomOf="@id/circle4"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textSize="28sp"
        android:id="@+id/sura9"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/circle1"
        app:layout_constraintEnd_toStartOf="@id/circle3"

        app:layout_constraintTop_toBottomOf="@id/circle5"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textSize="28sp"
        android:id="@+id/sura8"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/circle5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/circle6"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textSize="28sp"
        android:id="@+id/sura7"
        />




</androidx.constraintlayout.widget.ConstraintLayout>